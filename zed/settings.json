{
  "auto_update_extensions": {
    "catppuccin": false
  },
  "experimental.theme_overrides": {
    "border.variant": "#00000000"
  },
  "theme": "One Dark",
  "buffer_font_size": 17.0,
  "ui_font_size": 14,
  "vim_mode": true,
  "ui_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "relative_line_numbers": true,
  "tab_size": 2,
  "buffer_line_height": "standard",
  "indent_guides": {
    "enabled": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "tab_bar": {
    "show": false
  },
  "git": {
    "git_gutter": "hide",
    "inline_blame": {
      "enabled": false
    }
  },
  "project_panel": {
    "git_status": false
  },
  "scrollbar": {
    "git_diff": false
  }
}
